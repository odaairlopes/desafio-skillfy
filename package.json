    "test:debug": "jest --verbose",
    "test:watch": "jest --watch --silent",
    "build:pwa": "npm run build && npm run sw:generate",
    "sw:generate": "workbox generateSW workbox-config.js"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
